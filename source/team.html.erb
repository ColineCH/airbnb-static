<div class="container text-center padded">
    <h1>Our team</h1>
    <% members=["seb", "coline", "anne"] %>
    <ul class="list-inline">
    <% members.each do |member| %>
      <li>
        <h2>
          <%=member.capitalize%>
        </h2>
        <%= image_tag "#{member}.jpg", class: "avatar-large" %>
      </li>
    <% end %>
    </ul>
  <h3 class="padded">
   <!--  <a href="#"><i class="fa fa-mail"></i>contact@airbnb.com</a> -->
    <% link_to "mailto:contact@airbnb.com" do %>
      <i class="fa fa-envelope"></i> contact@airbnb.com
    <% end %>
  </h3>
</div>
